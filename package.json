{
  "name": "bouncerback",
  "version": "1.0.0",
  "description": "Bouncerback, a JS game built on React and MobX",
  "main": "index.js",
  "scripts": {
    "build-production": "NODE_ENV=production webpack",
    "build-development": "NODE_ENV=development webpack",
    "watch-development": "NODE_ENV=development webpack -w",
    "deploy": "node deploy",
    "phonegap-build-android-remote": "pgb login nacho@isegura.es && pgb update 3197157 && pgb build 3197157 android && pgb download 3197157 android ./builds",
    "phonegap-build-android-local": "npm run generate-icons && npm run copy-to-phonegap && cd ./bouncerapp && cordova build android",
    "copy-to-phonegap": "cp -a ./dev/. ./bouncerapp/www/",
    "phonegap-serve": "npm run copy-to-phonegap && cd ./bouncerapp && phonegap serve",
    "phonegap-emulate": "cd ./bouncerapp && cordova run --emulator",
    "generate-icons": "./node_modules/.bin/cordova-res-generator --platforms android --icon ./src/img/icon.png --splash ./src/img/splash.png --outputdir ./bouncerapp/res"
  },
  "author": "Nik Nak Studio (www.niknak.es). Soundtrack by Ignorance Is Strength (ignoranceisat.bandcamp.com)",
  "license": "ISC",
  "dependencies": {
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "breakpoint-sass": "^2.7.0",
    "cordova-res-generator": "^0.4.2",
    "css-loader": "^0.28.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^0.11.1",
    "ftp-deploy": "^2.1.1",
    "html-webpack-plugin": "^2.21.0",
    "install": "^0.10.0",
    "node-sass": "^4.9.0",
    "npm": "^6.14.6",
    "pgb-cli": "^1.0.2",
    "resolve-url": "^0.2.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "transformation-matrix": "^1.10.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.0"
  }
}
